{
  "name": "definition-stack",
  "displayName": "Definition Stack",
  "description": "Show definitions recursively in one tab.",
  "publisher": "mark-hahn",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.97.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "definition-stack.openwebview",
        "title": "Definition Stack: Open"
      },
      {
        "command": "definition-stack.closewebview",
        "title": "Definition Stack: Close"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "definition-stack.openwebview",
          "group": "navigation"
        }
      ],
      "commandPalette": [
        {
          "command": "definition-stack.openwebview"
        },
        {
          "command": "definition-stack.closewebview"
        }
      ]
    },
    "keybindings": [
      {
        "command": "definition-stack.openwebview",
        "key": "ctrl+shift+d",
        "mac": "cmd+shift+d"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.97.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.19.0"
  },
  "dependencies": {
    "prismjs": "^1.29.0",
    "pug": "^3.0.3",
    "reserved-words": "^0.1.2"
  }
}
